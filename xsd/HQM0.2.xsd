  <xs:simpleType name="Uniclass2015SingleCodeFormat">
    <!-- The uniclass format taks the form "letter letter, underscore, level 1 double digit number, underscore, level 2 double digit number, underscore, level 3 double digit number"-->
    <xs:annotation>
      <xs:documentation>This defines the expected format of the Uniclass Label, starts with two letters and has a repeatable "underscore and double digit number for up to four times </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([A-Z]{1}[a-zA-Z]{1}(_[0-9]{2})*){1}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Uniclass2015MultipleLabelsFormat">
    <!-- this definition requires further development to ensure it captures the format correctly-->
    <xs:annotation>
      <xs:documentation>Comma separated Uniclass Labels in the format Aa_00_01_02;Bb(_01_02_...);...;Zz(_01_02_...) </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([A-Z]{1}[a-zA-Z]{1}(_[0-9]{2})+){1}(;[A-Z]{1}[a-zA-Z]{1}(_[0-9]{2})+)*" />
      <!-- this definition requires further development to ensure it captures the format correctly -->
    </xs:restriction>
  </xs:simpleType>
