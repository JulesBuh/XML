  <xs:simpleType name="BuildScaleTypeLabel">
    <!-- A differentiator for groupings of dwellings which is essentially a flattened list which is derived from a number of grouping qualities such as number of dwellings and stacking of units, occupation of land and ownership-->
    <xs:annotation>
      <xs:documentation>the scale of the residential block</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="high rise">
        <xs:annotation>
          <xs:documentation>where the building height is more than equal to 18 meters or 5 storeys</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="low rise">
        <xs:annotation>
          <xs:documentation>where the building height is less than 18 meters or 6 storeys</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="bungalow">
        <xs:annotation>
          <xs:documentation>where the building height is less than 1 storey</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="town house">
        <xs:annotation>
          <xs:documentation>where the building height is less than 3 storeys and a single dwelling on a plot of land with parking provision on the ground floor within the footprint of the dwelling irrespective of whether the parking is within the building thermal envelope or outside it. </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
