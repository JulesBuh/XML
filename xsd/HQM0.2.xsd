<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/HQMXSDstylesheet.xsl"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.breeam.com/projects" xmlns:bre="http://www.breeam.com/projects" xmlns:hqm="http://www.homequalitymark.com" targetNamespace="http://www.breeam.com/projects" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="HQM0.2">
 
  <xs:element nillable="false" name="BREEAM">
    <xs:annotation>
      <xs:documentation>NOTE: This is a work in progress and is subject to change, whilst broader structure is relatively stable, more work is to be done in future on NameSpaces and finer details and positions of elements may be reconfigured at a future revision. The purposes of this schema is to provide industry with an assessment perspective structured document which is distributable and can be implemented by a third party who wishes to integrate it within their own processes or software. </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence minOccurs="0">
        <xs:element minOccurs="0" nillable="true" type="BREEAMSchemeStructure" name="Assessment" >
			<xs:annotation>
				<xs:documentation>Contains the HQM Assessment Fields</xs:documentation>
			</xs:annotation>
	</xs:element>
		<xs:element minOccurs="0" nillable="true" type="BREEAMSchemeManualStructure" name="Manual">
			<xs:annotation>
				<xs:documentation>Contains the HQM Technical Manual</xs:documentation>
			</xs:annotation>
	</xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- _____________________________Types_________________________________________________________________________________________ -->
  <xs:simpleType name="WholeNumberAboveZero">
    <!-- A simple basic definition allowing integers above zero-->
    <xs:annotation>
      <xs:documentation>a renamed (nonjargon) xml standard type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:positiveInteger" />
  </xs:simpleType>
  <xs:simpleType name="RatingType">
    <!-- assessment -->
    <!-- A numeric rating -->
    <xs:annotation>
      <xs:documentation>A numeric whole number scale, this is a "non negative integer" with no upper bound and a lower bound of zero</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ecoPointMeasure">
    <!-- issue -->
    <!-- A number renamed dedicated to ecoPoint derived from the Measure type-->
    <xs:annotation>
      <xs:documentation>Ecopoints are used as the metric for assessing this issue. It is an indicator that is made up of broad set of individual environmental indicators which are then combined into a single value. For more information about ecopoints, visit http://www.bre.co.uk/ or contact BRE.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="Measure" />
  </xs:simpleType>
  <xs:simpleType name="RSCSScoreMeasure">
    <!-- issue -->
    <!--BREEAM specific definition-->
    <xs:annotation>
      <xs:documentation>number assciated to the certification scheme as stated in BREEAM GN18</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0">
        <xs:annotation>
          <xs:documentation>0 is the lowest score for materials that are either uncertified or certified by a scheme that is not listed in GN18</xs:documentation>
        </xs:annotation>
      </xs:minInclusive>
      <xs:maxInclusive value="10">
        <xs:annotation>
          <xs:documentation>10 is the highest score for materials that are certified and listed in GN18</xs:documentation>
        </xs:annotation>
      </xs:maxInclusive>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="EIMMeasure">
    <!--BREEAM specific definition - host for a Measure and its associated unit-->
    <xs:annotation>
      <xs:documentation>A field that pairs the Ecopoint measure value with the valid unit for environmental impact</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="ecoPointMeasure">
        <xs:attribute name="unit">
          <xs:annotation>
            <xs:documentation>unit of Ecopointsmeasure</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <!-- issue -->
            <!--BREEAM specific definition-->
            <xs:annotation>
              <xs:documentation>Ecopoints are used as the metric for assessing this issue. It is an indicator that is made up of broad set of individual environmental indicators which are then combined into a single value.	For more information about ecopoints, visit http://www.bre.co.uk/ or contact BRE.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
              <xs:enumeration value="ecoPt">
                <xs:annotation>
                  <xs:documentation>ecopoints score</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="BREEAMdefinition">
    <!-- This creates the format for a definition that appears in the technical manual-->
    <xs:sequence>
      <xs:element minOccurs="0" nillable="true" name="term" type="term" />
      <!-- This is the title of the defintion but shouldn't be longer than 100 characters (this limitation has not been set within the schema)-->
      <xs:element minOccurs="0" nillable="true" name="definition" type="paragraph" />
      <!-- This is the paragraph description (no limitation has been placed and has the capacity for alternative languages)-->
      <xs:element minOccurs="0" nillable="true" name="reference" type="paragraph" />
      <!-- This is an additional field which gives potential for linking to a definition to a source. (multiple sources may be possible however the unbounded attibute would need to be added for this functionality)-->
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="BREEAMCNref">
    <!-- this definition requires further development to ensure it captures the format correctly-->
    <xs:annotation>
      <xs:documentation>This defines the expected format of the compliance notes code</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[C][N]([0-9]*)" />
      <!-- this is the reference number in the form "CN####2 -->
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREEAMmethodSequenceRef">
    <!-- this definition requires further development to ensure it captures the format correctly-->
    <xs:annotation>
      <xs:documentation>This defines the expected format of the step number</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[s][t][e][p].([0-9]*)" />
      <!-- this is the reference number in the form "step ##" -->
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREEAMcriterionRef">
    <!-- this definition requires further development to ensure it captures the format correctly-->
    <xs:annotation>
      <xs:documentation>This defines the expected format of criterion code </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[c][r][i][t].([0-9]*)" />
      <!-- this is the reference number in the form "crit ##" -->
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="BREEAMComplianceNote">
    <!-- This creates the format for a definition that appears in the technical manual-->
    <xs:sequence>
      <xs:element minOccurs="0" nillable="true" name="title" type="term" />
      <!-- This is the title of the defintion but shouldn't be longer than 100 characters (this limitation has not been set within the schema)-->
      <xs:element minOccurs="0" nillable="true" name="description" type="paragraph" />
      <!-- This is the paragraph description (no limitation has been placed and has the capacity for alternative languages)-->
      <xs:element minOccurs="0" nillable="true" name="reference" type="paragraph" />
      <!-- This is an additional field which gives potential for linking to a the note to a source. (multiple sources may be possible however the unbounded attibute would need to be added for this functionality)-->
      <xs:element minOccurs="0" maxOccurs="unbounded" nillable="true" name="application" type="term" />
      <!-- This is an additional field which gives potential for tagging a complaince note -->
    </xs:sequence>
    <xs:attribute name="id" type="BREEAMCNref" />
  </xs:complexType>
  <xs:complexType name="BREEAMMethod">
    <!-- This creates the format for a definition that appears in the technical manual-->
    <xs:sequence>
      <xs:element minOccurs="0" nillable="true" name="title" type="term" />
      <!-- This is the title of the defintion but shouldn't be longer than 100 characters (this limitation has not been set within the schema)-->
      <xs:element minOccurs="0" nillable="true" name="description" type="paragraph" />
      <!-- This is the paragraph description (no limitation has been placed and has the capacity for alternative languages)-->
      <xs:element minOccurs="0" nillable="true" name="reference" type="paragraph" />
      <!-- This is an additional field which gives potential for linking to a the note to a source. (multiple sources may be possible however the unbounded attibute would need to be added for this functionality)-->
      <xs:element minOccurs="0" nillable="true" name="application" type="term" />
      <!-- This is an additional field which gives potential for tagging a complaince note -->
    </xs:sequence>
    <xs:attribute name="id" type="BREEAMmethodSequenceRef" />
  </xs:complexType>
  <xs:complexType name="BREEAMcriterion">
    <!-- This creates the format for a definition that appears in the technical manual-->
    <xs:sequence>
      <xs:element minOccurs="0" nillable="true" name="title" type="term" />
      <!-- This is the title of the defintion but shouldn't be longer than 100 characters (this limitation has not been set within the schema)-->
      <xs:element minOccurs="0" nillable="true" name="description" type="paragraph" />
      <!-- This is the paragraph description (no limitation has been placed and has the capacity for alternative languages)-->
      <xs:element minOccurs="0" nillable="true" name="reference" type="paragraph" />
      <!-- This is an additional field which gives potential for linking to a the note to a source. (multiple sources may be possible however the unbounded attibute would need to be added for this functionality)-->
      <xs:element minOccurs="0" nillable="true" name="application" type="term" />
      <!-- This is an additional field which gives potential for tagging a complaince note -->
      <xs:element minOccurs="0" nillable="true" type="HQMIndicatorValues" name="indicators" />
    </xs:sequence>
    <xs:attribute name="creditsavailable" type="xs:integer" />
    <xs:attribute name="id" type="BREEAMcriterionRef" />
  </xs:complexType>
  <xs:complexType name="BREEAMevidence">
    <!-- This creates the format for a definition that appears in the technical manual-->
    <xs:sequence>
      <xs:element minOccurs="0" nillable="true" name="title" type="term" />
      <!-- This is the title of the defintion but shouldn't be longer than 100 characters (this limitation has not been set within the schema)-->
      <xs:element minOccurs="0" nillable="true" name="description" type="paragraph" />
      <!-- This is the paragraph description (no limitation has been placed and has the capacity for alternative languages)-->
      <xs:element minOccurs="0" nillable="true" name="reference" type="paragraph" />
      <!-- This is an additional field which gives potential for linking to a the note to a source. (multiple sources may be possible however the unbounded attibute would need to be added for this functionality)-->
      <xs:element minOccurs="0" nillable="true" name="application" type="term" />
      <!-- This is an additional field which gives potential for tagging a complaince note -->
      <xs:element minOccurs="0" nillable="true" name="BREEAMStage" type="projectStage" />
      <!-- This is an additional field which gives potential for associating evidence to project stage -->
      <xs:element minOccurs="0" nillable="true" name="documentation" type="documentationDefinition" />
      <!-- This is an additional field which gives potential for document parameters to be set -->
    </xs:sequence>
    <xs:attribute name="id" type="BREEAMcriterionRef" />
  </xs:complexType>
  <!-- classification -->
  <xs:simpleType name="Uniclass2015MultipleCodesFormat">
    <!-- this definition requires further development to ensure it captures the format correctly-->
    <xs:annotation>
      <xs:documentation>Comma separated Uniclass codes in the format Aa_00_01_02;Bb(_01_02_...);...;Zz(_01_02_...) </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([A-Z]{1}[a-zA-Z]{1}(_[0-9]{2})+){1}(;[A-Z]{1}[a-zA-Z]{1}(_[0-9]{2})+)*" />
      <!-- this definition requires further development to ensure it captures the format correctly -->
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Uniclass2015TemplateList">
    <!-- this is a template for where a pre-defined uniclass list is required. it is based on the generic uniclass expected format defined above, but limits it to a specific predetmerined list-->
    <xs:annotation>
      <xs:documentation>TemplateList for future development when Uniclass2015 is used</xs:documentation>
    </xs:annotation>
    <xs:restriction base="Uniclass2015SingleCodeFormat">
      <xs:enumeration value="Ss_10">
        <xs:annotation>
          <xs:documentation>Level 1 example</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Ss_10">
        <xs:annotation>
          <xs:documentation>Level 2 example</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Ss_10">
        <xs:annotation>
          <xs:documentation>Level 3 example</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Ss_10">
        <xs:annotation>
          <xs:documentation>Level 4 example</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREARNType">
    <!-- project -->
    <!-- This requires further development and would be key to webservice functionality -->
    <xs:annotation>
      <xs:documentation>The Report Reference Number is a 16-digit unique identifier assigned to each Assessment. 
			It is formatted as BREEAM-####-#### i.e. BREEAM-9999-9999 and assigned to a project upon registration of the assessment with BRE.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[B][R][E][E][A][M]-[0-9]{4}-[0-9]{4}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREEAMSchemeTitleType">
    <!-- assessment -->
    <!-- This will need to be maintained by BREEAM and expanded for BREEAMwide development. Proper thought and rational should be given to how the title is broken up for consistency. For example this list does not include the year or version number since these fields are likely to increment independently and catered for by an independent element -->
    <xs:annotation>
      <xs:documentation>A predefined list of acceptable BREEAM scheme names without the publication year</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Home Quality Mark">
        <xs:annotation>
          <xs:documentation>(New Build)Domestic Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BREEAM New Construction">
        <xs:annotation>
          <xs:documentation>non-Domestic Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BREEAM Domestic Refurbishment">
        <xs:annotation>
          <xs:documentation>Domestic Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BREEAM Infrastructure">
        <xs:annotation>
          <xs:documentation>Infrastructure Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BREEAM Communities">
        <xs:annotation>
          <xs:documentation>Communities Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREEAMBuildingType">
    <!-- project -->
    <!-- This is a broken down classification system independent of title, It is recommended that this should use the uniclass2015 Complexes list as its base or be mapped to this uniclass table in future development work -->
    <xs:restriction base="xs:string">
      <xs:enumeration value="non-Domestic">
        <xs:annotation>
          <xs:documentation>non-Domestic Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Domestic">
        <xs:annotation>
          <xs:documentation>Domestic Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Infrastructure">
        <xs:annotation>
          <xs:documentation>Infrastructure Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Communities">
        <xs:annotation>
          <xs:documentation>Communities Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREEAMSchemeLifeStageType">
    <!-- project -->
    <!-- This is a broken down classification system independent of title looking at the phase the assessment is aimed at. This will help distinguish a change of use assessment and criteria from that of new build. It is importnat  not to confuse this classification with scale as masterplanning as a title may suggest, hence using an industry accepted phasing code would be advisable (either RIBA 2013 PoW or Uniclass2 PP phases tables at L1 would be sufficient (or possible CEN TC 350 stages may be mapped)-->
    <xs:restriction base="xs:string">
      <xs:enumeration value="New_Construction">
        <xs:annotation>
          <xs:documentation>New Build</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Refurbishment">
        <xs:annotation>
          <xs:documentation>Refurbishment Domestic Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Planning">
        <xs:annotation>
          <xs:documentation>Masterplanning Scheme</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREEAMAssessmentStageType">
    <!-- assessment -->
    <!-- This is a broken down classification for an active assessment, i.e. when an assessment is being caried out, the RIBA PoW job stages can be mapped to the BREEAM terminology of Pre-Assessment, Design Stage and Post construction Stage as they are broad terms that span across very specific work stages of a project. NOTE This is differentiated from task based perspectives which may be more granular and not fit within any particular stage. At criterion level care should be taken for whether it is a task based requirement or a time based requirement -->
    <xs:annotation>
      <xs:documentation>BREEAM Stages of Assessment</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Pre-Assessment" ><xs:annotation>
          <xs:documentation> possible at RIBA work stages 0-4</xs:documentation>
        </xs:annotation></xs:enumeration>
      
      <xs:enumeration value="Design" ><xs:annotation>
          <xs:documentation> possible at RIBA work stages 1-4</xs:documentation>
        </xs:annotation></xs:enumeration>
      <xs:enumeration value="Post-Construction" ><xs:annotation>
          <xs:documentation> possible RIBA work stages 4+ (i.e preparation for some criteria could begin at stage 4 and LOD and LOI is at level 6 for the items being assessed)</xs:documentation>
        </xs:annotation></xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREEAMEvidenceType">
    <!-- assessment -->
    <!-- This will need to be maintained by BREEAM and expanded for BREEAMwide development to reflect GN18. Proper thought and rational should be given to how the title is broken up for consistency. For example this list does not include the year or version number since these fields are likely to increment independently and catered for by an independent element -->
    <xs:annotation>
      <xs:documentation>A predefined list of evidence</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="graphical specification">
        <xs:annotation>
          <xs:documentation>drawings sections plans isometrics, annotated visuals</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="graphical visualisation">
        <xs:annotation>
          <xs:documentation>drawings of an illustrative nature with less definitive information, such as a sketch, artist impression etc.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="movie clip">
        <xs:annotation>
          <xs:documentation>a graphical document that includes information presented over a sequence enabling the representation of time</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="analytical graphics">
        <xs:annotation>
          <xs:documentation>a graphical document that includes additional metric information about the subject of study and includes a scale that quantifies the visual coding in the graphic. e.g thermal imaging, topography gradients, daylight analysis</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="written specification">
        <xs:annotation>
          <xs:documentation>Documents with definitive written information</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="manufacturer's literature">
        <xs:annotation>
          <xs:documentation>Documents with definitive written information</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="contract information">
        <xs:annotation>
          <xs:documentation>Legally binding information</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="schedule">
        <xs:annotation>
          <xs:documentation>Documents itemised in a consistent format presented in a table</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="programme">
        <xs:annotation>
          <xs:documentation>A specific type of schedule with timeline information included spanning a period of time</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="checklist">
        <xs:annotation>
          <xs:documentation>An itemised list with an action or series of choices against it</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="minutes">
        <xs:annotation>
          <xs:documentation>Documentation that records a log of decisions and actions at a particular event and participants present at the event</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="employer's requirements">
        <xs:annotation>
          <xs:documentation>A document produced at an early stage prescribing the requirements of the building</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="certificate">
        <xs:annotation>
          <xs:documentation>Certification that communicate the performance or credntials of the item that has been certificated</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="unlisted">
        <xs:annotation>
          <xs:documentation>This is a hidden item to allow extension for new schemes.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="evidenceType">
    <xs:union memberTypes="BREEAMEvidenceType listExtension" />
  </xs:simpleType>
  <xs:simpleType name="relationshipType">
    <!-- assessment -->
    <!-- This will need to be maintained by BREEAM and expanded for BREEAMwide development to reflect GN18. Proper thought and rational should be given to how the title is broken up for consistency. For example this list does not include the year or version number since these fields are likely to increment independently and catered for by an independent element -->
    <xs:annotation>
      <xs:documentation>A predefined list of relationship</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="1st party">
        <xs:annotation>
          <xs:documentation>such as a self declaration</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2nd party">
        <xs:annotation>
          <xs:documentation>such as a trade declaration</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3rd party">
        <xs:annotation>
          <xs:documentation>such as an accreditation body</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <!-- masterlists for BREEAM should be created in a  hollistic manner. future developemnt should rationalise this further for consistency -->
  <xs:simpleType name="BREEAMSections">
    <!-- section -->
    <!-- This requires maintenance from BREEAM strategy teams. this should contain all possible titles and names for all schemes. from which subset can be constructed for the scheme structure -->
    <xs:annotation>
      <xs:documentation>List of Sections in HQM</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <!-- Suggested Shared Titles -->
      <xs:enumeration value="The Context" />
      <!-- suggested titles for BREEAM if this level is adopted elsewhere -->
      <xs:enumeration value="The Building" />
      <!-- suggested titles for BREEAM if this level is adopted elsewhere -->
      <xs:enumeration value="The Interior" />
      <!-- suggested titles for BREEAM if this level is adopted elsewhere -->
      <xs:enumeration value="Project Management" />
      <!-- suggested titles for BREEAM if this level is adopted elsewhere -->
      <!-- assessment -->
      <!-- HQM Titles -->
      <xs:enumeration value="Our Surroundings" />
      <xs:enumeration value="My Home" />
      <xs:enumeration value="Knowledge Sharing" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BREEAMCategories">
    <!-- category -->
    <!-- This requires maintenance from BREEAM strategy teams. this should contain all possible titles and names for category levels of all schemes. from which subset can be constructed for the scheme structure -->
    <xs:annotation>
      <xs:documentation>List of Categories in HQM</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <!-- Shared Titles -->
      <xs:enumeration value="Energy" ><xs:annotation>
          <xs:documentation> HQM Issue 2.02</xs:documentation>
        </xs:annotation></xs:enumeration>
      <xs:enumeration value="Materials" ><xs:annotation>
          <xs:documentation> HQM Issue 2.03</xs:documentation>
        </xs:annotation></xs:enumeration>
      <xs:enumeration value="Management" ><xs:annotation>
          <xs:documentation> HQM Issue 3.01</xs:documentation>
        </xs:annotation></xs:enumeration>
      <xs:enumeration value="Health and Wellbeing" ><xs:annotation>
          <xs:documentation> HQM Wellbeing Indicator</xs:documentation>
        </xs:annotation></xs:enumeration>
      <xs:enumeration value="Ecology" ><xs:annotation>
          <xs:documentation> HQM Issue 1.03</xs:documentation>
        </xs:annotation></xs:enumeration>
      <xs:enumeration value="Transport" ><xs:annotation>
          <xs:documentation> HQM Issue 1.02</xs:documentation>
        </xs:annotation></xs:enumeration>
      <xs:enumeration value="Water" ><xs:annotation>
          <xs:documentation> HQM Issue 2.04</xs:documentation>
