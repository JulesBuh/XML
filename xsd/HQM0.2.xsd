  <xs:simpleType name="NRMCodeFormat">
    <!-- The NRM Label format takes the form "number, full-stop, number, full-stop, single or double digit number" which essentially expresses three levels of definition for building elements or systems. It essentially is a flattened list of what uniclass distills into two separate tables both of which possess their own hierarchy-->
    <xs:annotation>
      <xs:documentation>This defines the expected format of the NRM Label, starts with a number,and has a repeatable has a repeatable decimal place followed by a single or double digit imbetween for up to two times </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{1}(\.[0-9]{1,2})*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="NRMCodeMultipleFormat">
    <!-- this definition requires further development to ensure it captures the format correctly-->
    <xs:annotation>
      <xs:documentation>This defines the expected format of the NRM Label, starts with a number,and has a repeatable decimal place followed by a single or double digit imbetween for up to two times. This pattern can be repeated for multipl NRM Labels separated by a semicolon </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]{1}(\.[0-9]{1,2}){2} [a-zA-Z0-9.]*)(;[0-9]{1}(\.[0-9]{1,2}){2} ([a-zA-Z0-9.])*)+" />
      <!-- this definition requires further development to ensure it captures the format correctly-->
    </xs:restriction>
  </xs:simpleType>
