  <xs:complexType name="Uniclass2015Structure">
    <!-- this is combining tables with associated meta data which defines a uniclass table. This forms a complete defintion for a uniclass list when used as a reference. This type can be mapped to the tables that are eriodically published for easy updates see https://toolkit.thenbs.com/articles/classification#classificationtables-->
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="title">
        <xs:complexType>
          <xs:annotation>
            <xs:documentation>The structure of Uniclass2015 list includes a code, a parent code, a group code, a sub-group code, a section code, an object code and an NBS and NRM mapping code</xs:documentation>
          </xs:annotation>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attribute name="parentCode" type="Uniclass2015SingleCodeFormat">
                <xs:annotation>
                  <xs:documentation>Uniclass parent code</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="code" type="Uniclass2015SingleCodeFormat">
                <xs:annotation>
                  <xs:documentation>Uniclass full code</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="group" type="xs:integer">
                <xs:annotation>
                  <xs:documentation>Uniclass group code</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="sub-group" type="xs:integer">
                <xs:annotation>
                  <xs:documentation>Uniclass sub-group code</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="section" type="xs:integer">
                <xs:annotation>
                  <xs:documentation>Uniclass category code</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="object" type="xs:integer">
                <xs:annotation>
                  <xs:documentation>Uniclass object code</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="NRMCode" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Uniclass object code</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="NBSCode" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Uniclass object code</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="table" type="xs:string">
      <xs:annotation>
        <xs:documentation>A placeholder to describe the name of the UniclassTable e.g 2015Ss Systems</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="version" type="xs:string">
      <xs:annotation>
        <xs:documentation>a placeholder to describe the uniclass version number e.g v1.1</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="date" type="xs:date">
      <xs:annotation>
        <xs:documentation>a placeholder to describe the uniclassTable release date e.g 2015-09-29</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
